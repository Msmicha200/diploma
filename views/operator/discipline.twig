{% macro tableDiscipline(discipline) %}
	<tr data-disciplineid="{{ discipline.Id }}">
		<td>{{ discipline.Id }}</td>
		<td class="editable">{{ discipline.Title }}</td>
    	<td align="center">
    		<button type="button" class="edit-discipline">
				<img src="/images/edit.svg" alt="">
    		</button>
    	</td>
	</tr>
{% endmacro %}

{% macro disciplineSelect(discipline) %}
	<li class="uvm--option discipline ds-group-option" data-disciplineid="{{ discipline.Id }}">
		{{ discipline.Title }}
	</li>
{% endmacro %}
